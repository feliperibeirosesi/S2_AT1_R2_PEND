let numero = Math.floor(Math.random() * 10) + 1; // Gera um número aleatório entre 1 e 10.
let R; // Variável para armazenar a resposta do usuário.

do {
    R = parseFloat(prompt("Resposta: ")); // Pede para o usuário responder.
} while (R !== numero); // Continua pedindo até o usuário acertar.

console.log("Você acertou!"); // Exibe a mensagem quando o usuário acerta o número.
